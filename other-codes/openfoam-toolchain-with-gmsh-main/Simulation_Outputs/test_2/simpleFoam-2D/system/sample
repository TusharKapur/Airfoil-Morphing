FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      sample;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

type surfaces;
libs            ("libsampling.so");

interpolationScheme cellPoint;

surfaceFormat       raw;

surfaces
(
    main
    {
        type      patch;
        patches   (bladenlf26_bladenlf26);
        triangulate	false;
    }
    
    flap
    {
        type      patch;
        patches   (bladenlf26_bladenlf26FLAP);
        triangulate	false;
    }
    
);

fields          (p);

