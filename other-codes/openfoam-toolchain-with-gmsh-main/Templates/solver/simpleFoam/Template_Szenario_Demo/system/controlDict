FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     _VARapplication_;

startFrom       startTime;

startTime       _VARstartTime_;

stopAt          endTime;

endTime         _VARendTime_;

deltaT          _VARdeltaT_;

writeControl    _VARwriteControl_;

writeInterval   _VARwriteIntervall_;

purgeWrite      _VARpurgeWrite_;

writeFormat     ascii;

writePrecision  _VARwritePrecision_;

writeCompression _VARwriteCompression_;

timeFormat      _VARtimeFormat_;

timePrecision   _VARtimePrecision_;

runTimeModifiable _VARrunTimeModifiable_;


